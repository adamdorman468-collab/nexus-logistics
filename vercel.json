{
    "version": 2,
    "framework": null,
    "functions": {
        "api/*.php": { "runtime": "vercel-php@0.6.2" }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://${VERCEL_URL}",
        
        "APP_CONFIG_CACHE": "/tmp/config_v2.php",
        "APP_EVENTS_CACHE": "/tmp/events_v2.php",
        "APP_PACKAGES_CACHE": "/tmp/packages_v2.php",
        "APP_ROUTES_CACHE": "/tmp/routes_v2.php", 
        "APP_SERVICES_CACHE": "/tmp/services_v2.php",
        "VIEW_COMPILED_PATH": "/tmp",
        
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}